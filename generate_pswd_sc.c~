const uint CHAR_OFFSET = 32;
const uint CHARS = 95;

bool crack(char* str, uint pos, uint size){
  for(uint i = 0; i < CHARS; ++i){
    str[pos] = CHAR_OFFSET + i;
    if(pos + 1 < size){
      if(crack(str, pos + 1, size))
        return true;
    }
    else if(check(str, size))  
      return true;
  }
  return false;
}

